{
  "manifest_version": 2,
  "name": "Tabby - new tab",
  "description": "This extension replaces default new tab in chrome",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "bookmarks"
  ],
  "chrome_url_overrides": {
    "newtab": "tabby.html"
  }
}